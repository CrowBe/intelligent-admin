# Frontend Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_VERSION=v1

# WebSocket/SSE Configuration
VITE_WEBSOCKET_URL=ws://localhost:3000
VITE_SSE_URL=http://localhost:3000

# Application Configuration
VITE_APP_NAME="Intelligent Assistant"
VITE_APP_VERSION=0.1.0
VITE_APP_DESCRIPTION="AI-powered administrative assistant for trade businesses"

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_ANALYTICS=false

# Third-party Services
VITE_SENTRY_DSN=your-sentry-dsn
VITE_GOOGLE_ANALYTICS_ID=your-ga-id

# File Upload Configuration
VITE_MAX_FILE_UPLOAD_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,txt,jpg,jpeg,png

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true